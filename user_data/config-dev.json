
{
    "$schema": "https://schema.freqtrade.io/schema.json",
    "max_open_trades": 5,
    "stake_currency": "USDT",
    "stake_amount": 1000.0,
    "tradable_balance_ratio": 0.99,
    "amend_last_stake_amount": true,
    "last_stake_amount_min_ratio": 0.1,
    "timeframe": "1h",
    "fiat_display_currency": "USD",
    "dry_run": false,
    "dry_run_wallet": 10000,
    "cancel_open_orders_on_exit": false,
    "process_only_new_candles": false,
    "minimal_roi": {
        "0": 10
    },
    "stoploss": -0.9,
    "fee": 0.001,
    "trading_mode": "futures",
    "margin_mode": "isolated",
    "ignore_roi_if_entry_signal": true,
    "unfilledtimeout": {
        "entry": 10,
        "exit": 10,
        "exit_timeout_count": 0,
        "unit": "minutes"
    },
    "entry_pricing": {
        "price_side": "other",
        "use_order_book": true,
        "order_book_top": 3,
        "price_last_balance": 0.0,
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1
        }
    },
    "exit_pricing":{
        "price_side": "other",
        "use_order_book": true,
        "order_book_top": 1,   
        "price_last_balance": 0.0,
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1
        }
    },
    "use_exit_signal": true,
    "exit_profit_only": false,
    "order_types": {
        "entry": "market",
        "emergency_exit": "market",
        "stoploss": "market",
        "exit": "market",
        "stoploss_on_exchange": true,
        "stoploss_order_type": "market"
    },
    "exchange": {
        "name": "okx",
        "key": "insert_key",
        "secret": "insert_secret",
        "password":"insert_password",
        "ccxt_config": {
             "options": {
                "sandbox": true,
                "defaultType": "swap"
            }
        },
        "ccxt_async_config": {
            "options": {
                "sandbox": true,
                "defaultType": "swap"
            }
        },
        "pair_whitelist": [
            "BTC/USDT:USDT"
        ],
        "pair_blacklist": [
        ]
    },
    "pairlists": [
        {"method": "StaticPairList"}
    ],
    "telegram": {
        "enabled": false,
        "token": "insert_token",
        "chat_id": "insert_chat_id"
    },
    "api_server": {
        "enabled": false,
        "listen_ip_address": "0.0.0.0",
        "listen_port": 8080,
        "verbosity": "error",
        "enable_openapi": false,
        "jwt_secret_key": "",
        "ws_token": "",
        "CORS_origins": [],
        "username": "admin",
        "password": "admin"
    },
    "bot_name": "freqtrade",
    "initial_state": "running",
    "force_entry_enable": false,
    "internals": {
        "process_throttle_secs": 15
    },
    "gpt": {
        "model": "insert_model",
        "api_key": "insert_api_key",
        "base_url": "insert_base_url",
        "system_prompt_file": "user_data/strategies/prompt/v1.txt",
        "mock_signal": "long",
        "memory_file": "user_data/memory.dev.json",
        "interval_minutes": 60,
        "force_interval_minutes": 15
    }
}